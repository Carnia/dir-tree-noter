{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "Materialize": true
    },
    "rules": {
        "react/prop-types": 0,
        "no-unused-vars": 0,
        "no-console": 1,
        "semi": [
            2,
            "always"
        ]
    }
}
